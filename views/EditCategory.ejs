<script>
      function isValidName(name) {
  // Regular expression that checks if the name contains only letters and spaces
        const nameRegex = /^[A-Za-z\s]+$/;
        return nameRegex.test(name);
    }


      function validate() {
    let username = document.getElementById("name");
    let useremail = document.getElementById("email");
    let userpassword = document.getElementById("password");
    const usernumber = document.getElementById("phone");

    // Get the entered value
    let enteredValue = usernumber.value.trim();

    // Check if the entered value is a valid number
    if (isNaN(enteredValue) || enteredValue === "") {
        alert("Please enter a valid number");
        return false;
    }

    let trimmedUsername = username.value.trim();
    let trimmedUserpassword = userpassword.value.trim();

    
    if (trimmedUsername === "") {
        alert("Please fill username input");
        return false;
    } else if (trimmedUsername === "" && useremail.value === "" && trimmedUserpassword === "" && enteredValue === "") {
        alert("Please fill all the fields");
        return false;
    } else if (useremail.value === "") {
        alert("Please fill email input");
        return false;
    } else if (trimmedUserpassword === "") {
        alert("Please fill password input");
        return false;
    } else if (trimmedUsername.length <= 3 || trimmedUserpassword.length <= 4 || trimmedUserpassword.length >= 15) {
        alert("Please add more length to your name and password");
        return false;
    }
    else if (!isValidName(trimmedUsername)) {
    alert("Please enter a valid name without numbers");
    return false;
  }
    else if (trimmedUsername.length >=20) {
        alert("Name is too lengthy");
        return false;
    } else if (enteredValue.length >=11) {
        alert("phone number should be 10 digit");
        return false;
    }else if (enteredValue.length <10) {
        alert("phone number should be 10 digit");
        return false;
    } else {
        return true;
    }
}

  </script>





